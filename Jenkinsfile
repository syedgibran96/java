pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Jenkins running the build stage ...'
           
            }
        }
      stage('test') {
            steps {
                echo 'Jenkins running the test stage ...'
           
            }
        }
      stage('deploy') {
            steps {
                echo 'Jenkins running the deploy stage ...'
           
            }
        }
    }
}
